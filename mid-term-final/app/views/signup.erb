<link href="../..css/form.css" rel="stylesheet">


<div class="container padnav form-column">
  <form action="signup" class="form-signin" method="POST">
    <h2 class="form-signin-heading">Sign Up</h2>
    <p>It's free and takes less than 30 seconds.</p>
    
    <label class=>First name:</label>
    <input name="first_name" class="form-control">

    <label>Last Name:</label>
    <input name="last_name" class="form-control">

    <label>Username:</label>
    <input name="username" class="form-control">

    <label>Password:</label>
    <input name="password" class="form-control">

    <label>Email:</label>
    <input name="email" class="form-control">

    <label>Phone:</label>
    <input name="phone" class="form-control">

    <button class="btn btn-lg btn-danger btn-block" type="submit">Sign Up</button>
  </form>
</div>
